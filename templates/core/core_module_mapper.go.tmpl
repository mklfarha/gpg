package {{.Package}}

import (    
    entity "{{.ProjectIdentifier}}/core/entity/{{.Package}}"
    {{.ProjectIdentifier}}db "{{.ProjectIdentifier}}/core/repository/gen"  
	{{if eq .HasArrayField true}}
		"{{.ProjectIdentifier}}/core/entity/mapper"
	{{end}}
	{{range $import := .Imports}} {{if ne $import "time"}} 
    "{{$import}}" {{end}}{{end}}
)

func mapModelsToEntities(models []{{.ProjectIdentifier}}db.{{.EntityName}}) []entity.{{.EntityName}} {
	result := []entity.{{.EntityName}}{}
	for _, p := range models {
		result = append(result, mapModelToEntity(p))
	}
	return result
}

func mapModelToEntity(model {{.ProjectIdentifier}}db.{{.EntityName}}) entity.{{.EntityName}} {	
	return entity.{{.EntityName}}{ {{range $field := .Fields}}
        {{$field.Name}}: {{$field.RepoFromMapper}},{{end}}	
	}
}